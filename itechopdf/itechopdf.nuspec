<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>itechopdf</id>
    <version>1.0.4-alpha</version>
    <title>ItechoPdf</title>
    <authors>Willem Bijker,Jaco van Rensburg</authors>
    <owners>Willem Bijker,Jaco van Rensburg</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>PDF creation library around WkHtmlToPdf. Creates rich PDF's from HTML, CSS and javascript. Include cover pages, header and footers with a simple API.</description>
    <tags>pdf html wkhtmltopdf</tags>
    <repository type="git" url="https://github.com/itechodev/itechopdf" />
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="HtmlAgilityPack" version="1.11.19" exclude="Build,Analyzers" />
        <dependency id="PDFClown" version="0.1.2" exclude="Build,Analyzers" />
        <dependency id="System.Drawing.Common" version="4.7.0" exclude="Build,Analyzers" />
        <dependency id="System.Text.Encoding.CodePages" version="4.7.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>


  </metadata>
   
   <files>
    <file src="itechopdf.targets" target="Build\" />
  
    <file src="build\libwkhtmltox.dll" target="Build\" />
    <file src="build\libwkhtmltox.dylib" target="Build\" />
    <file src="build\libwkhtmltox.so" target="Build\" />
      
  </files>
     
</package>